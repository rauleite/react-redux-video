{
  "name": "react-redux-starter-kit",
  "version": "3.0.0-alpha.2",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "start": "yarn install && yarn deploy:prod && yarn start-server",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "compile:prod": "better-npm-run compile",
    "compile:server": "better-npm-run compile:server",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start-server": "better-npm-run start-server",
    "dev": "better-npm-run dev",
    "dev:server": "better-npm-run dev:server",
    "dev:debug": "better-npm-run dev:debug",
    "dev:node": "better-npm-run dev:node",
    "test": "better-npm-run test",
    "test:debug": "better-npm-run test:debug",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "js-tests": "babel-node js-tests.js"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile && npm run compile:server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "compile:prod": {
      "command": "NODE_ENV=production node bin/compile && npm run compile:server",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "compile:server": {
      "command": "babel server --out-dir dist-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "DEBUG=express* nodemon bin/dev-server.js --ignore node_modules --ignore dist --ignore coverage --ignore tests --ignore src --exec babel-node",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "dev:server": {
      "command": "NODE_ENV=production nodemon bin/dev-server.js --ignore node_modules --ignore dist --ignore coverage --ignore tests --ignore src --exec babel-node",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "dev:debug": {
      "command": "nodemon --debug-brk=5858 bin/dev-server.js --ignore node_modules --ignore dist --ignore coverage --ignore tests --ignore src --exec babel-node",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "dev:node": {
      "command": "nodemon index_old.js --ignore node_modules --ignore dist --ignore coverage --ignore tests --ignore src --exec babel-node",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start-server": {
      "command": "NODE_ENV=production DEBUG=express* node bin/dev-server.js",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "test:debug": {
      "command": "nodemon --debug-brk=5858 ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "bcrypt": "^1.0.2",
    "better-npm-run": "0.0.13",
    "compression": "^1.6.2",
    "css-loader": "^0.26.4",
    "cssnano": "^3.10.0",
    "debug": "^2.2.0",
    "deep-freeze": "^0.0.1",
    "express-limiter": "^1.6.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "fs-extra": "^2.0.0",
    "global": "^4.3.1",
    "helmet": "^3.5.0",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.8.1",
    "imports-loader": "^0.7.1",
    "ip": "^1.1.2",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.3.0",
    "material-ui": "^0.17.0",
    "mongoose": "^4.8.4",
    "node-sass": "^4.5.0",
    "nodemailer": "^3.1.3",
    "normalize.css": "^5.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "postcss-loader": "^1.3.3",
    "react": "^15.4.2",
    "react-async-script": "^0.8.0",
    "react-dom": "^15.4.2",
    "react-google-recaptcha": "^0.8.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "redis": "^2.7.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.2",
    "tiny-promisify": "^1.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "yargs": "^6.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "codecov": "^1.0.1",
    "enzyme": "^2.0.0",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.1.1",
    "express": "^4.15.2",
    "karma": "^1.0.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack-with-fast-source-maps": "^1.9.2",
    "mocha": "^3.0.1",
    "nodemon": "^1.10.2",
    "phantomjs-prebuilt": "^2.1.12",
    "react-addons-test-utils": "~15.4.2",
    "redbox-react": "^1.2.10",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  }
}
